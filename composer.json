{
  "name": "richardhj/contao-onlinetickets",
  "description": "Online tickets extension for Isotope eCommerce",
  "type": "contao-module",
  "authors": [
    {
      "name": "Richard Henkenjohann",
      "email": "richard@henkenjohann.me",
      "homepage": "http://henkenjohann.me",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "richard@henkenjohann.me"
  },
  "require": {
    "php": "^5.4 || ^7.0",
    "isotope/isotope-core": "^2.3",
    "cliffparnitzky/user-member-bridge": "dev-master"
  },
  "autoload": {
    "psr-0": {
      "OnlineTicket\\": "src/"
    }
  },
  "extra": {
    "contao": {
      "sources": {
        "entrypoint-api": "api",
        "assets": "assets/tickets",
        "module": "system/modules/onlinetickets"
      }
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/richardhj/contao-user-member-bridge.git"
    }
  ]
}
